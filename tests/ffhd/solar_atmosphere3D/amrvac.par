 &filelist
    base_filename='output/fs3d'
    autoconvert = F
    saveprim=T
    convert_type='vtuBCCmpi'
 /

 &savelist
    itsave(1,1)=0
    itsave(1,2)=0
    dtsave_log=1.0d0
    dtsave_dat=1.0d0
 /
    itsave(1,1)=0
    itsave(1,2)=0
    dtsave_log=0.1d0

 &stoplist
    it_max=10
 /

 &methodlist
    time_stepper='threestep'
    flux_scheme=20*'tvdlf'
    limiter=20*'vanleer'
    phys='ffhd'
 /

 &boundlist
    specialboundary=T
    typeboundary_min1 = 4*'cont'
    typeboundary_max1 = 4*'cont'
    typeboundary_min2 = 4*'cont'
    typeboundary_max2 = 4*'cont'
    typeboundary_min3 = 4*'special'
    typeboundary_max3 = 4*'cont'
 /

 &meshlist
    max_blocks=4096
    block_nx1=16
    block_nx2=16
    block_nx3=16
    domain_nx1=256
    domain_nx2=256
    domain_nx3=256
    xprobmin1=-8.d0
    xprobmax1=-2.d0
    xprobmin2=-3.d0
    xprobmax2=3.d0
    xprobmin3=0.d0
    xprobmax3=6.d0
 /

 &paramlist
    typecourant='maxsum'
    courantpar=0.6d0
    slowsteps=10
 /

 &ffhd_list
    ffhd_pdivb=T
    ffhd_hyperbolic_thermal_conduction=T
    ffhd_radiative_cooling=T
    ffhd_gravity=T
    ffhd_source_usr=F
 /

 &rc_list
    ncool=4000
    coolcurve='Colgan_DM'
    coolmethod='exact'
    Tfix=.true.
    tlow=0.008d0
 /

 &usr_list
    dh = 4.4d0
    Bh = 8.d0
    dv = 1.d0
    Bv = 25.d0
    xv = -5.d0
    dp1 = 0.d0
 /

