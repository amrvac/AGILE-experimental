
 &filelist
   base_filename    = 'output/jet'
   autoconvert      = F
   convert_type     = 'vtuBCCmpi'
   saveprim         = T
 /

 &savelist
   itsave(1,1)      = 0
!   itsave(1,2)      = 0
!   ditsave_log      = 10
!   ditsave_dat      = 100

 /

 &stoplist
   it_max           = 100
 /

 &methodlist
   time_stepper    = 'threestep'
   flux_scheme     = 20*'tvdlf'
   limiter         = 20*'vanleer'
   phys            = 'hd'
 /

 &boundlist
   typeboundary_min1 = 5*'periodic'
   typeboundary_max1 = 5*'periodic'
   typeboundary_min2 = 5*'periodic'
   typeboundary_max2 = 5*'periodic'
   typeboundary_min3 = 5*'periodic'
   typeboundary_max3 = 5*'periodic'
 /

 &meshlist
   max_blocks         = 64
   refine_max_level   = 1
   block_nx1          = 16
   block_nx2          = 16
   block_nx3          = 16
   domain_nx1         = 64
   domain_nx2         = 64
   domain_nx3         = 64
   xprobmin1          = -1.0d0
   xprobmax1          =  1.0d0
   xprobmin2          = -1.0d0
   xprobmax2          =  1.0d0
   xprobmin3          =  0.0d0
   xprobmax3          =  2.0d0
   iprob=1
 /

 &paramlist
   slowsteps        = 10
   courantpar       = 0.7d0
 /
