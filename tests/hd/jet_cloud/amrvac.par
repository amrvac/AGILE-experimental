
 &filelist
   base_filename    = 'output/jet'
   autoconvert      = T
   convert_type     = 'vtuBCCmpi'
   saveprim         = T
 /

 &savelist
   itsave(1,1)      = 0
   itsave(1,2)      = 0
   ditsave_log      = 100
!   ditsave_dat      = 100

 /

 &stoplist
   time_max        = 10.0d0
 /

 &methodlist
   time_stepper    = 'threestep'
   flux_scheme     = 20*'tvdlf'
   limiter         = 20*'vanleer'
   phys            = 'hd'
 /

 &boundlist
   typeboundary_min1 = 5*'cont'
   typeboundary_max1 = 5*'cont'
   typeboundary_min2 = 5*'noinflow'
   typeboundary_max2 = 5*'noinflow'
   typeboundary_min3 = 5*'noinflow'
   typeboundary_max3 = 5*'noinflow'
 /

 &meshlist
   max_blocks       = 1024
   refine_max_level = 1
   block_nx1        = 16
   block_nx2        = 16
   block_nx3        = 16
   domain_nx1       = 256
   domain_nx2       = 128
   domain_nx3       = 128
   xprobmin1        = -16.0d0
   xprobmax1        = 16.0d0
   xprobmin2        = -8.0d0
   xprobmax2        = 8.0d0
   xprobmin3        = -8.0d0
   xprobmax3        = 8.0d0
 /

 &paramlist
   slowsteps        = 10
   courantpar       = 0.8d0
 /
